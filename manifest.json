{
  "name": "Laterman",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Save things for later, man.",
  "homepage_url": "https://github.com/soulprovidr/laterman",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Laterman"
  },
  "chrome_url_overrides": {
    "newtab": "src/index.html"
  },
  "permissions": [
    "storage"
  ]
}
